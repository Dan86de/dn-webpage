{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog/?$",
			"dest": "/_isr?x_astro_path=$0"
		}
	],
	"images": {
		"sizes": [
			344
		],
		"minimumCacheTTL": 60,
		"formats": [
			"image/webp"
		],
		"domains": [],
		"remotePatterns": []
	}
}